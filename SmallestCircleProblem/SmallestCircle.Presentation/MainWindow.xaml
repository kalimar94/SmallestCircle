<Window x:Class="SmallestCircle.Presentation.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SmallestCircle.Presentation"
        mc:Ignorable="d"
        Title="MainWindow" MinHeight="400" MinWidth="400" Width="800" Height="800">
    <Grid>
            
        <TabControl x:Name="tabControl">
            <TabItem Header="Random Points">
                <local:RandomizedGeneration />
            </TabItem>
            <TabItem Header="ScratchPad">
                <local:ScratchPad />
            </TabItem>
        </TabControl>
    </Grid>
</Window>
